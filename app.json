{
  "expo": {
    "name": "Costeo360",
    "slug": "Costeo360",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon360.png",
    "scheme": "costeo360",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon360.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Costeo360"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      "expo-localization",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supportsRTL": false,
      "forcesRTL": false,
      "router": {
              "initialRouteName": "(tabs)",
              "notFound": {
                "content": "Pantalla no encontrada",
                "title": "404",
                "statusCode": 404
              }
            },
      "eas": {
        "projectId": "7542f8f7-bccc-4fb9-b743-c0465f634f75"
      }
    }
  }
}
